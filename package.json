{
  "name": "todo-react",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jgerigmeyer/todo-react.git"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.5",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype-errors": "^3.6.0",
    "eslint-plugin-prettier": "^2.6.1",
    "eslint-plugin-react": "^7.10.0",
    "flow-bin": "^0.75.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.6",
    "stylelint": "^9.3.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.1.3"
  },
  "dependencies": {
    "accoutrement-init": "^1.1.2",
    "autoprefixer": "^8.6.3",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "node-sass": "^4.9.0",
    "parcel-bundler": "^1.9.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0"
  },
  "scripts": {
    "precommit": "lint-staged",
    "start": "parcel src/index.html",
    "prettier": "prettier --write *.js .*.js src/js/**/*.js src/sass/**/*.scss",
    "eslint": "eslint *.js .*.js src/js/**/*.js",
    "stylelint": "stylelint src/sass/**/*.scss",
    "lint": "yarn run prettier; yarn run eslint; yarn run stylelint"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint",
      "git add"
    ],
    "*.scss": [
      "prettier --write",
      "stylelint",
      "git add"
    ]
  }
}
