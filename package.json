{
  "name": "todo-react",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jgerigmeyer/todo-react.git"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.5",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-flowtype-errors": "^3.6.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-prettier": "^2.6.1",
    "eslint-plugin-react": "^7.10.0",
    "flow-bin": "^0.75.0",
    "flow-types": "^1.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.6",
    "stylelint": "^9.3.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.1.3",
    "webpack": "^4.15.1",
    "webpack-command": "^0.4.1"
  },
  "dependencies": {
    "@salesforce-ux/design-system": "^2.6.2",
    "@salesforce/design-system-react": "^0.8.18",
    "accoutrement-init": "^1.1.2",
    "autoprefixer": "^8.6.3",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "css-loader": "^1.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "node-sass": "^4.9.0",
    "postcss-loader": "^2.1.5",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1"
  },
  "scripts": {
    "precommit": "lint-staged",
    "start": "parcel serve src/index.html --open --no-autoinstall",
    "prettier": "prettier --write *.js .*.js src/js/**/*.js src/sass/**/*.scss",
    "eslint": "eslint *.js src/js/**/*.js",
    "stylelint": "stylelint src/sass/**/*.scss",
    "lint": "yarn run prettier; yarn run eslint; yarn run stylelint",
    "prod": "parcel build src/index.html --out-dir dist/prod/"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint",
      "git add"
    ],
    "*.scss": [
      "prettier --write",
      "stylelint",
      "git add"
    ]
  }
}
